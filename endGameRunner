package ttreImages;

import javax.swing.*;
import java.awt.*;

public class endGameRunner extends JFrame {
    private static final int WIDTH = 1920;
    private static final int HEIGHT = 1080;

    public endGameRunner(String framename, int v1, int v2, int v3, int v4, int v5, int v6, int v7, int v8, int v9) {
        super(framename);
 

        setSize(WIDTH, HEIGHT);
        setResizable(false);

        //setLocationRelativeTo(null);

        setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);

        add(new EndGame(v1, v2, v3, v4, v5, v6, v7, v8, v9));

        setVisible(true);
    }
}
